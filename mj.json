        ],
        "parameters": [
          {
            "in": "path",
            "name": "cod_organismo",
            "required": true,
            "description": "código del organismo emisor (Ej. 111042 para ingeniería). Obtener código desde http://front-test6402.uchile.cl/siaper-json/diccionario/servicio.json",
